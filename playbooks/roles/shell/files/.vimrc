syntax enable

set background=dark
"let g:solarized_termcolors=256
"let g:solarized_contrast="high"
"colorscheme solarized

" show line numbers
"set number
" highlight cursor line
"set cursorline

" searchi
" search as you type
set incsearch
" highlight matches
set hlsearch

set foldenable

" tab size
set tabstop=4
" tab size for editing
set softtabstop=4
" use spaces to represent tabs
set expandtab
